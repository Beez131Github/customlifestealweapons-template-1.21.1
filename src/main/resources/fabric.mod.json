{
	"schemaVersion": 1,
	"id": "customlifestealweapons",
	"version": "1.0.1",
	"name": "Custom Lifesteal Weapons",
	"description": "A mod that adds custom weapons for lifesteal gameplay.",
	"authors": [
		"Beez131"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/customlifestealweapons/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"adapter": "default",
				"value": "net.beez131.customlifestealweapons.Customlifestealweapons"
			}
		],
		"fabric-datagen": [
			{
				"adapter": "default",
				"value": "net.beez131.customlifestealweapons.CustomlifestealweaponsDataGenerator"
			}
		],
		"client": [
			{
				"adapter": "default",
				"value": "net.beez131.customlifestealweapons.CustomLifestealweaponsClient"
			}
		]
	},
	"mixins": [
		"customlifestealweapons.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "1.21.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}
